      <script type="text/javascript">
        $(function () {
            $(".keyword").show();
            $(".search").hide();
            $(".title").change(function () {
              var title = $(".title").is(":checked");
                if (title) {
                  $(".search").show();
                  $(".keyword").hide();
                }
                else {
                  $(".search").hide();
                  $(".keyword").show();
                }
            });
        });
    </script>
